{
  "name": "ihme-ui",
  "description": "Visualization tools from the Institute for Health Metrics and Evaluation",
  "version": "0.19.6",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "dist",
    "test-utils",
    "ui",
    "utils",
    "style",
    "index.js",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "contributors": [
    {
      "name": "Gabe Medrash",
      "email": "gmedrash@gmail.com"
    },
    {
      "name": "Brett Vitaz",
      "email": "vitazbt@uw.edu"
    }
  ],
  "repository": "https://github.com/ihmeuw/ihme-ui.git",
  "bugs": "https://github.com/ihmeuw/ihme-ui/issues",
  "scripts": {
    "lint": "$(npm bin)/eslint src --ext .js,.jsx",
    "lint:fix": "$(npm bin)/eslint src --fix --ext .js,.jsx --quiet",
    "coverage": "BABEL_ENV=test $(npm bin)/babel-node $(npm bin)/babel-istanbul cover $(npm bin)/_mocha --require ./scripts/setup-jsdom.js -- `scripts/get-test-directories.sh`",
    "coverage:travis": "BABEL_ENV=test $(npm bin)/babel-node $(npm bin)/babel-istanbul cover $(npm bin)/_mocha --require ./scripts/setup-jsdom.js --report lcovonly -- `scripts/get-test-directories.sh`",
    "coverage-token": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "demo": "BABEL_ENV=demo scripts/bundle-demos.sh",
    "test": "BABEL_ENV=test $(npm bin)/babel-node $(npm bin)/_mocha --require ./scripts/setup-jsdom.js `./scripts/get-test-directories.sh`",
    "clean": "rm -rf dist test-utils ui utils style index.js",
    "combine-css": "scripts/concat-stylesheets.sh",
    "minify-css": "$(npm bin)/cssnano dist/ihme-ui.css dist/ihme-ui.min.css",
    "build:commonjs": "NODE_ENV=production BABEL_ENV=commonjs:babel $(npm bin)/babel --ignore test/,demo src --out-dir . && npm run combine-css",
    "build:umd": "NODE_ENV=production BABEL_ENV=commonjs:webpack webpack --config webpack.build.config.js src/index.js dist/ihme-ui.js",
    "build:umd:min": "NODE_ENV=production BABEL_ENV=commonjs:webpack MINIFY=1 webpack --config webpack.build.config.js src/index.js dist/ihme-ui.min.js && npm run minify-css",
    "build:all": "npm run clean && npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "travis-test": "npm run lint && npm run coverage:travis && npm run coverage-token",
    "preversion": "npm run lint && npm run test",
    "version": "npm run build:all && git add .",
    "postversion": "git push && git push --tags",
    "webpack:aster": "./node_modules/.bin/webpack --config src/ui/aster-chart/demo/webpack.config.js --watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-css-modules-transform": "^0.2.0",
    "babel-plugin-lodash": "^3.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.3.13",
    "block-loader": "^2.1.0",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.4.0",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.23.1",
    "cssnano-cli": "^1.0.4",
    "d3-request": "^0.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "jsdom": "^8.1.0",
    "mocha": "^2.3.4",
    "postcss": "^5.1.2",
    "postcss-loader": "^0.8.2",
    "react-addons-test-utils": "^15.0.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "webpack": "2.1.0-beta.22",
    "webpack-notifier": "^1.3.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.3",
    "d3": "^4.2.2",
    "ihme-react-select": "git+https://github.com/ihmeuw/ihme-react-select.git",
    "interact.js": "~1.2",
    "lodash": "^4.5.0",
    "react": "^15.1.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-addons-shallow-compare": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-faux-dom": "^2.4.0",
    "react-virtualized": "^7.0.5",
    "topojson": "^1.6.24"
  }
}
